{
  "name": "pulseplan-server",
  "version": "1.0.0",
  "description": "Backend server for PulsePlan app with Google Calendar integration",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:clean": "pkill -f 'ts-node src/index.ts' || true && npm run dev",
    "dev:port": "lsof -ti:5000 | xargs kill -9 || true && npm run dev",
    "build": "tsc",
    "watch": "tsc --watch",
    "ports": "lsof -i :5000-5010",
    "test:n8n": "ts-node src/scripts/test-n8n-agent.ts"
  },
  "keywords": [
    "pulseplan",
    "google-calendar",
    "oauth",
    "calendar-integration"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/msal-node": "1.18.0",
    "@google/generative-ai": "^0.21.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@supabase/supabase-js": "^2.49.4",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "@upstash/redis": "^1.28.2",
    "axios": "^1.10.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "googleapis": "^133.0.0",
    "lru-cache": "^10.1.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.98.0",
    "puppeteer": "^23.8.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-url-polyfill": "^2.0.0",
    "resend": "^4.6.0",
    "socket.io": "^4.8.1",
    "url-parse": "^1.5.10",
    "uuid": "^11.1.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "@types/puppeteer": "^7.0.4",
    "@types/redis": "^4.0.11",
    "@types/url-parse": "^1.4.11",
    "@types/validator": "^13.12.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
